using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class spawning : MonoBehaviour
{
    public GameObject cube;
    public int n;
    void Start()
    {
        for(int i=0; i<n;i++)
        Instantiate(cube, new Vector3(Random.Range(-1f,1f), 1,Random.Range(-7f, 7f)), Quaternion.identity);
    }

    void Update()
    {
        
    }
}
